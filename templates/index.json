    "custom_html_P3rLaQ": {
      "type": "custom-html",
      "name": "Custom HTML",
      "settings": {
        "html": "<style>\n/* Essential carousel styling */\n.custom-carousel {\n  width: 100%;\n  overflow: hidden;\n  margin: 0;\n  padding: 0;\n}\n\n.custom-carousel .slick-slide {\n  margin: 0;\n  padding: 0;\n  height: 150px;\n}\n\n.custom-carousel img {\n  width: 150px;\n  height: 150px;\n  object-fit: cover;\n  margin: 0;\n}\n\n/* Override the default slick animation */\n.custom-carousel .slick-list {\n  overflow: visible !important; /* This makes exiting slides visible outside the container */\n}\n\n/* This ensures the container remains clipped despite overflow:visible on slick-list */\n.custom-carousel-wrapper {\n  overflow: hidden;\n  width: 100%;\n  position: relative;\n}\n\n/* Hide unloaded images */\n.custom-carousel .slick-slide:not(.slick-active) img {\n  visibility: hidden;\n}\n</style>\n\n<link rel=\"stylesheet\" type=\"text/css\" href=\"https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css\">\n<link rel=\"stylesheet\" type=\"text/css\" href=\"https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css\">\n<script src=\"https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js\"></script>\n\n<!-- Added wrapper div to control overflow -->\n<div class=\"custom-carousel-wrapper\">\n  <div class=\"custom-carousel\">\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Leb.jpg?v=1746172125\" alt=\"NSW Office\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Daycare.jpg?v=1746172125\" alt=\"Layered Map Art\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Grand.jpg?v=1746172125\" alt=\"New Zealand Contour Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/ColoradoMap2.jpg?v=1746249751\" alt=\"Colorado Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/FarmMap1.jpg?v=1746249750\" alt=\"Farm Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/UlladullaMap.jpg?v=1746249750\" alt=\"Ulladulla Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/LebanonWine_be8649f5-bcf1-4a3b-9da7-57fa10108bd7.jpg?v=1746249750\" alt=\"Lebanon Wine Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/BonnieDoon.jpg?v=1746249750\" alt=\"Bonnie Doon Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Arkansas2.jpg?v=1746249751\" alt=\"Arkansas Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/QLDMap.jpg?v=1746249750\" alt=\"Queensland Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/WashingtonMap.jpg?v=1746249750\" alt=\"Washington Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Iran.jpg?v=1746172125\" alt=\"Idaho State Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Arkansas.jpg?v=1746172125\" alt=\"NSW Office Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Syd.jpg?v=1746172125\" alt=\"Map Art Bedroom Decor\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Office.jpg?v=1746172125\" alt=\"Iceland Topographic Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/QLDD.jpg?v=1746172125\" alt=\"Arkansas 3D Carved Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Idaho.jpg?v=1746172125\" alt=\"Iran Home Decor\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/PNG2.jpg?v=1746172125\" alt=\"PNG Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Iceyy.jpg?v=1746172125\" alt=\"Iceland Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/PNG.jpg?v=1746172125\" alt=\"PNG Home Decor\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Icey.jpg?v=1746172125\" alt=\"Iceland Home Decor\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Offfice.jpg?v=1746172125\" alt=\"Office Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Toilet.jpg?v=1746172125\" alt=\"Bathroom Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Cali.jpg?v=1746172125\" alt=\"California Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Philly.jpg?v=1746172125\" alt=\"Philadelphia Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/BEd.jpg?v=1746172125\" alt=\"Bedroom Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/NZ.jpg?v=1746172125\" alt=\"New Zealand Map\"></div>\n    <div><img src=\"https://cdn.shopify.com/s/files/1/0308/6709/5685/files/Beachhosue.jpg?v=1746172125\" alt=\"Beach House Map\"></div>\n  </div>\n</div>\n\n<script>\n$(document).ready(function(){\n  // Initialize carousel\n  var $carousel = $('.custom-carousel');\n  \n  $carousel.slick({\n    infinite: true,\n    slidesToShow: 7,\n    slidesToScroll: 1,\n    autoplay: true,\n    autoplaySpeed: 750,\n    arrows: false,\n    dots: false,\n    pauseOnHover: false,\n    pauseOnFocus: false,\n    cssEase: 'linear',\n    speed: 500,\n    responsive: [\n      {\n        breakpoint: 768, // Tablets\n        settings: {\n          slidesToShow: 3\n        }\n      },\n      {\n        breakpoint: 480, // Mobiles\n        settings: {\n          slidesToShow: 3\n        }\n      }\n    ]\n  });\n  \n  // Preload images\n  function preloadImages() {\n    var slidesToPreload = 14; // Current + next set\n    var currentIndex = $carousel.slick('slickCurrentSlide');\n    \n    for (var i = 0; i < slidesToPreload; i++) {\n      var nextIndex = (currentIndex + i) % $('.slick-slide').length;\n      var $slide = $('.slick-slide').eq(nextIndex);\n      var $img = $slide.find('img');\n      if ($img.length) {\n        var imgSrc = $img.attr('src');\n        $('<img>').attr('src', imgSrc); // Preload by creating a hidden image\n      }\n    }\n  }\n  \n  // Initial preload\n  preloadImages();\n  \n  // Preload on slide change\n  $carousel.on('beforeChange', function() {\n    preloadImages();\n  });\n  \n  // Make slides visible as they exit\n  $carousel.find('.slick-slide').css('visibility', 'visible');\n  \n  // Resume autoplay after user interaction\n  $carousel.on('mouseup touchend', function() {\n    $carousel.slick('slickPlay');\n  });\n});\n</script>",
        "show_padding": true
      }
